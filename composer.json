{
    "name": "phossa/phossa-weixin",
    "license": "MIT",
    "type": "library",
    "description": "Implementation of Weixin API",
    "keywords": ["phossa", "weixin", "qq"],
    "homepage": "https://github.com/phossa/phossa-weixin",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa"
        }
    ],
    "autoload": {
        "psr-0": { "Phossa\\Weixin\\": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Phossa\\Weixin\\": "tests/src/" }
    },
    "require": {
        "php": ">=5.4.0",
        "phossa/phossa-shared": "*"
    },
    "require-dev": {
